{
    "name": "@mayd/contentful-adapter",
    "version": "4.1.0",
    "license": "UNLICENSED",
    "homepage": "https://github.com/Becklyn-Studios/mayd-contentful-adapter",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Becklyn-Studios/mayd-contentful-adapter.git"
    },
    "scripts": {
        "test": "jest",
        "format": "prettier --write \"**/*.{ts,tsx,json}\"",
        "lint": "prettier --check \"**/*.{ts,tsx,json}\"",
        "build": "npm run clean && tsc && chmod +x lib/bin/mayd-contentful.js",
        "clean": "rm -rf lib",
        "prepublishOnly": "npm test && npm run lint && npm run build",
        "preversion": "npm run lint"
    },
    "files": [
        "lib/**/*",
        "setup/**/*"
    ],
    "main": "lib/src/index.js",
    "types": "lib/src/index.d.ts",
    "bin": {
        "mayd-contentful": "lib/bin/mayd-contentful.js"
    },
    "dependencies": {
        "@contentful/rich-text-html-renderer": "^15.11.1",
        "@contentful/rich-text-types": "^15.11.1",
        "@mayd/ui-types": "^4.1.1",
        "contentful": "^9.1.6",
        "contentful-management": "^7.51.5",
        "contentful-migration": "^4.7.0",
        "dotenv": "^14.3.2",
        "glob": "^7.2.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "safe-json-stringify": "^1.2.0",
        "ts-node": "^10.4.0",
        "tsconfig-paths": "^3.12.0"
    },
    "devDependencies": {
        "@becklyn/prettier": "^1.0.2",
        "@types/glob": "^7.2.0",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.10",
        "@types/react-dom": "^17.0.9",
        "@types/safe-json-stringify": "^1.1.2",
        "jest": "^27.5.1",
        "prettier": "^2.4.1",
        "ts-jest": "^27.1.4",
        "typescript": "^4.4.3"
    },
    "prettier": "@becklyn/prettier"
}
